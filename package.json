{
  "name": "@immutablesoft/form-edit",
  "version": "0.1.2",
  "description": "FormAlly form editor for ReactJS, creates .json definitions for rendering with form-render.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ImmutableSoft/form-edit.git"
  },
  "keywords": [
    "form",
    "creator",
    "json",
    "definition",
    "payment",
    "image",
    "attachment",
    "file",
    "date",
    "signature",
    "editor"
  ],
  "author": "ImmutableSoft Inc.",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
      "/dist"
  ],
  "bugs": {
    "url": "https://github.com/ImmutableSoft/form-edit/issues"
  },
  "homepage": "https://github.com/ImmutableSoft/form-edit#readme",
  "main": "./dist/lib.umd.js",
  "module": "./dist/lib.module.js",
  "source": "src/lib.package.js",
  "peerDependencies": {
    "react": "0.14 - 18"
  },
  "devDependencies": {
    "microbundle": "^0.15.1"
  },
  "scripts": {
    "build": "microbundle --jsx React.createElement --jsxFragment React.Fragment --jsxImportSource react",
    "build:lib": "microbundle --jsx React.createElement --jsxFragment React.Fragment --jsxImportSource react",
    "dev": "microbundle watch --jsx React.createElement --jsxFragment React.Fragment --jsxImportSource react"
  }
}
